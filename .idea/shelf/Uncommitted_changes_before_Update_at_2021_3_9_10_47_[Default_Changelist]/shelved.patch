+"""本文获取联动云致联盟的_csrf参数值"""
+import requests
+import  re
+
+
+RE = 'name="_csrf" value="(.+?)"/>'
+HOST='http://testerp-zlm.ldygo.com'
+PATH='/login'
+
+html = requests.post(HOST+PATH).text.encode('unicode_escape').decode("utf8")
+a = re.findall(RE,html)
+print(a[0])
+
+
+def get__csrf(text):
+    """
+        正则匹配获取联动云致联盟的_csrf参数值
+    :param text: 网页html-text
+    :return:
+    """
+    RE = 'name="_csrf" value="(.+?)"/>'
+    result = re.findall(RE, str(text))
+    return result[0] if result else None
+
+
+def get_jsessionid(text):
+    """
+    切割获取联动云致联盟的登录cookie：JSESSIONID
+    :param text:  登录成功接口返回的url
+    :return:JSESSIONID=***
+    """
+    return 'JSESSIONID='+str(text).split(";")[1].split("=")[1]
+if __name__ == '__main__':
+    # print((get__csrf("123")))
+    print(get_jsessionid('http://testerp-zlm.ldygo.com/welcome;jsessionid=3DE9B2F39544DC67CC98CD0DA4F2E2DA'))
